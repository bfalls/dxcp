{
  "application": "dxcp-demo",
  "name": "rollback-demo-service",
  "description": "Rollback demo-service to previous artifact",
  "keepWaitingPipelines": false,
  "limitConcurrent": true,
  "parameterConfig": [
    {
      "name": "service",
      "required": true
    },
    {
      "name": "version",
      "required": true
    },
    {
      "name": "engineUrl",
      "required": true
    }
  ],
  "stages": [
    {
      "type": "dxcpRollback",
      "name": "rollback-artifact",
      "refId": "1",
      "requisiteStageRefIds": [],
      "payload": {
        "service": "${ parameters.service }",
        "version": "${ parameters.version }"
      }
    }
  ]
}
